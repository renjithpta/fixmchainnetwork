{
  "name": "fixmtoacriscontract",
  "version": "0.0.1",
  "description": "My Smart Contract",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'functionalTests/**/*.ts'",
    "pretest": "npm run lint",
    "test": "nyc mocha -r functionalTests/AcrisDataContract-fixmtoacriscontract@0.0.1.test.ts",
    "start": "fabric-chaincode-node start",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "engineStrict": true,
  "author": "Sethu Raman",
  "license": "Apache-2.0",
  "dependencies": {
    "class-converter": "^1.2.0",
    "deepmerge": "^4.2.2",
    "fabric-contract-api": "^2.2.0",
    "fabric-network": "^2.2.9",
    "fabric-shim": "^1.4.6",
    "iterate-object": "^1.3.4",
    "long": "^4.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.3",
    "@types/sinon": "^7.5.2",
    "@types/sinon-chai": "^3.2.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.0",
    "typescript": "^3.9.10"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage/**",
      "dist/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
